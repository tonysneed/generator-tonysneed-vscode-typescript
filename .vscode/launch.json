{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug TypeScript Tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/mocha/bin/mocha",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/"
		},
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "/usr/local/bin/yo",
			"stopOnEntry": false,
			"args": [
                "tonysneed-vscode-typescript",
                "--skip-install"
            ],
			"cwd": "${workspaceRoot}/",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"sourceMaps": false
		}
	]
}
